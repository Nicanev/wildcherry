{
  "name": "wildcherry",
  "version": "1.0.0",
  "description": "website of the international marketplace of clothing, footwear, electronics, children's goods, household goods and other goods.",
  "source": "src/pug/index.pug",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "parcel build --no-scope-hoist",
