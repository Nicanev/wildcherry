  "license": "ISC",
  "targets": {
    "web": {
      "context": "browser",
      "publicUrl": "/wildcherry/"
    }
  },
  "devDependencies": {
    "@parcel/transformer-pug": "^2.7.0",
    "@parcel/transformer-sass": "^2.7.0",
    "@parcel/transformer-vue": "^2.7.0",
    "parcel": "^2.7.0",
    "sharp": "^0.29.3"
  },
  "dependencies": {
    "gh-pages": "^4.0.0",
    "jquery": "^3.6.1",
    "jquery-ui-dist": "^1.13.2",
    "swiper": "^8.3.2",
    "vue": "^3.2.38"
  }
